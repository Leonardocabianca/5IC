<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphicsServer.ServerController">
   <children>
      <TextArea fx:id="textArea" editable="false" layoutX="24.0" layoutY="102.0" prefHeight="203.0" prefWidth="552.0" wrapText="true" />
      <Label layoutX="24.0" layoutY="64.0" prefHeight="31.0" prefWidth="116.0" text="Log text">
         <font>
            <Font name="Raleway Light" size="18.0" />
         </font>
      </Label>
      <Pane layoutY="335.0" prefHeight="65.0" prefWidth="600.0" style="-fx-background-color: #e5e5e5;">
         <children>
            <Button fx:id="start" layoutX="240.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#startServer" text="START" />
            <Button fx:id="close" layoutX="307.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#closeServer" text="CLOSE" />
         </children>
      </Pane>
      <Pane fx:id="statusPane" prefHeight="6.0" prefWidth="600.0" />
      <Label alignment="CENTER" layoutX="-2.0" layoutY="20.0" prefHeight="38.0" prefWidth="604.0" text="ADMINISTRATOR">
         <font>
            <Font name="Raleway Medium" size="36.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
